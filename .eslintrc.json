{
  "extends": ["brightspace/polymer-config"],
  "parserOptions": {
    "ecmaVersion": 6
  },
  "env": {
    "browser": true,
    "es6": true
  },
  "plugins": [
    "html"
  ],
  "rules": {
    "brace-style": "off",
    "new-cap": "off",
    "no-var": "off",
    "require-jsdoc": "off"
  },
  "globals": {
    "Polymer": true
  }
}
